{
  "name": "n8n-nodes-woovi",
  "version": "0.3.0",
  "description": "n8n nodes for Woovi",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Eduardo Maciel de Matos",
    "email": "eduardo.maciel@woovi.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/woovibr/n8n-nodes-woovi.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc -p tsconfig.build.json && gulp build:icons",
    "dev": "tsc -p tsconfig.build.json --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes credentials package.json",
    "test": "jest"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/WooviApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Woovi/Woovi.node.js",
      "dist/nodes/Woovi/WooviTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@swc/core": "^1.11.24",
    "@swc/jest": "^0.2.38",
    "@types/callsite": "^1.0.34",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.3",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-core": "^1.89.0",
    "n8n-nodes-base": "^1.89.0",
    "nock": "^14.0.4",
    "prettier": "^3.3.2",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "vite": "^6.3.5",
    "vitest": "^3.1.3"
  },
  "dependencies": {
    "callsites": "3",
    "jest-mock-extended": "4.0.0-beta1",
    "n8n-workflow": "^1.89.0",
    "sqlite3": "^5.1.7"
  }
}
